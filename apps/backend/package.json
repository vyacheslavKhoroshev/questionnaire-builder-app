    "cors": "^2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "knex": "^3.1.0",
    "pg": "8.11.5",
    "zod": "3.23.8",
    "convict": "^7.2.4"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.14.10",
    "@types/pg": "8.11.6",
    "tsx": "^4.19.1",
    "typescript": "5.5.4"
  }
}
